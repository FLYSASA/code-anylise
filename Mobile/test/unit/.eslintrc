{
    "env": {
        "jasmine": true,
        "browser": true
    },
    "plugins": [
        "jasmine"
    ],
    //"extends": "plugin:jasmine/recommended",
    "rules": {
        "jasmine/no-focused-tests": 2,
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        // allow paren-less arrow functions
        "arrow-parens": 0,
        // allow async-await
        "generator-star-spacing": 0
    }
}